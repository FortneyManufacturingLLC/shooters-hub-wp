{
  "name": "shooters-hub-match-finder-plugin",
  "private": true,
  "version": "1.0.1",
  "scripts": {
    "clean:src-js": "find src -maxdepth 1 -name '*.js' -delete",
    "sync:finder-ui": "node ./scripts/sync-finder-ui.mjs",
    "prebuild": "npm run sync:finder-ui && npm run clean:src-js",
    "build": "vite build",
    "smoke-test": "./scripts/ci-smoke-test.sh",
    "release:verify": "npm run build && npm run smoke-test",
    "bump-version": "node scripts/bump-version.mjs"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "leaflet": "^1.9.4",
    "leaflet.markercluster": "^1.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.11",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  }
}
